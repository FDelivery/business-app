<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.fdelivery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FDelivery">
        <activity android:name=".newDelivery"></activity>
        <activity android:name=".DeliveryTable" />
        <activity android:name=".DeliveryHistory" />
        <activity android:name=".RegisterNewBusiness" />
        <activity
            android:name=".BusinessMain"
            android:theme="@style/Theme.FDelivery.NoActionBar">

        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.FDelivery.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>