        android:layout_width="116dp"
        android:layout_height="94dp"
        app:srcCompat="@drawable/deliveryman"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="16dp" />


