        android:layout_width="132dp"
        android:layout_height="106dp"
        app:srcCompat="@drawable/deliveryman"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />


